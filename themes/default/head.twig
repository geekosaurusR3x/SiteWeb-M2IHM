    </script>
    <!--script google analytics -->
</head>
<body>
	<header id="header">
		<div class="inner clearfix">
			<h1><a href="{{ base_url }}">{{ site_title }}</a></h1>
			<ul class="nav">
				<!--{% for page in pages %}
				<li><a href="{{ page.url }}">{{ page.title }}</a></li>
				{% endfor %}-->
                {{ at_navigation.navigation }}
			</ul>
		</div>
	</header>
